<VBox alignment="CENTER_LEFT" prefHeight="587.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unoeste.fipp.sistemafx.EmpresaController">
